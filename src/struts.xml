<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="nosshv1" namespace="/" extends="struts-default">
		<action name="login" class="noname.nosshv1.struts.LoginAction">
			<result name="success">/success.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		<action name="add" class="noname.nosshv1.struts.AddAction">
			<result name="success">/account_add.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		<action name="update" class="noname.nosshv1.struts.UpdateAction">
			<result name="success">/account_update.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		<action name="index" class="noname.nosshv1.struts.IndexAction">
			<result name="success">/index.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		<action name="accountadd" class="noname.nosshv1.struts.AccountAddAction">
			<result name="success">/success.jsp</result>
		</action>
		<action name="accountupdate" class="noname.nosshv1.struts.AccountUpdateAction">
			<result name="success">/success.jsp</result>
		</action>
		<action name="accountdelete" class="noname.nosshv1.struts.AccountDeleteAction">
			<result name="success">/success.jsp</result>
		</action>
	</package>
</struts>
